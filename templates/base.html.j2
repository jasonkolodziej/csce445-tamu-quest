            <div class="container">
                <div class="media-container-row content text-white">
                    <div class="col-12 col-md-3">
                        <div class="media-wrap">
                            <a href="{{ url_for('home') }}">
                                <img src="{{ url_for('static',filename='images/logo_transparent.png') }}" alt="Mobirise">
                            </a>
                        </div>
                    </div>
                    <div class="col-12 col-md-3 mbr-fonts-style display-7">
                        <h5 class="pb-3">
                            Members of Team STAT
                        </h5>
                        <p class="mbr-text">
                            Jeslyn Duran
                            <br>Rhonda Hollier
                            <br>Madi Panzarella
                        </p>
                    </div>
                    <div class="col-12 col-md-3 mbr-fonts-style display-7">
                        <h5 class="pb-3">
                        <br>
                        </h5>
                        <p class="mbr-text">
                            Jason Kolodziej
                            <br>Ben McKenzie
                            <br>Ben Mora
                        </p>
                    </div>
                </div>
                <div class="footer-lower">
                    <div class="media-container-row">
                        <div class="col-sm-12">
                            <hr>
                        </div>
                    </div>
                    <div class="media-container-row mbr-white">
