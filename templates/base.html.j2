        <link rel="stylesheet" href="{{ url_for('static',filename='bootstrap/css/bootstrap-reboot.min.css') }}">
        <link rel="stylesheet" href="{{ url_for('static',filename='tether/tether.min.css') }}">
        <link rel="stylesheet" href="{{ url_for('static',filename='dropdown/css/style.css') }}">
        <link rel="stylesheet" href="{{ url_for('static',filename='socicon/css/styles.css') }}">
        <link rel="stylesheet" href="{{ url_for('static',filename='theme/css/style.css') }}">
        <link rel="preload" as="style" href="{{ url_for('static',filename='mobirise/css/mbr-additional.css') }}">
        <link rel="stylesheet" href="{{ url_for('static',filename='mobirise/css/mbr-additional.css') }}" type="text/css">

    </head>
    <body>
        <section class="menu cid-qTkzRZLJNu" once="menu" id="menu1-0">
            <nav class="navbar navbar-expand beta-menu navbar-dropdown align-items-center navbar-fixed-top navbar-toggleable-sm">
                <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <div class="hamburger">
                        <span></span>
                        <span></span>
                        <span></span>
                        <span></span>
                    </div>
                </button>
                <div class="menu-logo">
                    <div class="navbar-brand">   
                        <span class="navbar-caption-wrap"><a class="navbar-caption text-white display-4" href="{{url_for('home')}}">Super Texas A&amp;M Trivia</a></span>
                    </div>
                </div>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav nav-dropdown" data-app-modern-menu="true"><li class="nav-item">
                            <a class="nav-link link text-white display-4" href="{{ url_for('home') }}"><span class="mobi-mbri mobi-mbri-home mbr-iconfont mbr-iconfont-btn"></span>
                                Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link link text-white display-4" href="{{url_for('instructions')}}"><span class="mobi-mbri mobi-mbri-help mbr-iconfont mbr-iconfont-btn"></span>
                                How to Play?</a>
                        </li></ul>
                    <div class="navbar-buttons mbr-section-btn"><a class="btn btn-sm btn-success display-4" href="{{ url_for('game') }}"><span class="mobi-mbri mobi-mbri-play mbr-iconfont mbr-iconfont-btn"></span>
                            Start Trivia</a></div>
                </div>
            </nav>
        </section>
        {% block child_container %}{% endblock child_container%}
